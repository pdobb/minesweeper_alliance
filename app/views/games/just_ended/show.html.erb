<%# locals: (view:) %>

<%= turbo_frame_tag(view.turbo_frame_name) do %>
  <div class="space-y-12">
    <%= render("games/just_ended/content", content: view.content) %>

    <div class="container mx-auto">
      <%= render(
            "games/just_ended/footer",
            footer: view.footer(user: current_user)) %>
    </div>
  </div>
<% end %>
